    "build:prod": "env-cmd -f ./.prod.env npm run-script build",
    "start:prod": "env-cmd -f ./.prod.env npm run-script start",
    "deploy:windows": "xcopy build\\* C:\\xampp\\htdocs\\audit-manager\\ /E /H /C /I",
    "deploy:linux": "cp -r build/* /var/www/html/audit-manager/",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start"
  },
